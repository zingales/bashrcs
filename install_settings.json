{
  "_comment": "defualts: install_dir= ~, install name = .filename, save_original = true, to_ignore = false",
  "vim_colors": {
    "install_dir": "~/.vim/colors"
  },
  "scripts": {
    "install_dir": "~/scripts"
  },
  "to_save": [".gitconfig"],
  "to_ignore": [".DS_Store"],
  "git_ensure": [{
    "folder": "~/.gat",
    "url": "https://github.com/zingales/gat.git"
  }],
  "cd_install": [{
    "folder": "~/.gat",
    "cmd": "cp ex_gat_script ~/scripts/gat",
    "force": true,
    "name": "gat"
  }]
}